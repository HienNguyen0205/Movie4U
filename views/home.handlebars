<div id="main_carousel" class="carousel carousel-dark slide" data-bs-ride="carousel">
    <div class="carousel-inner">
        {{#each carousel}}
            {{#if this.index}}
            <div class="carousel-item">
                <img src="{{this.src}}" class="d-block w-100" alt="..." data-bs-interval="6000">
            </div>
            {{else}}
            <div class="carousel-item active">
                <img src="{{this.src}}" class="d-block w-100" alt="..." data-bs-interval="6000">
            </div>
            {{/if}}
        {{/each}}
    </div>
</div>
<div class="container py-5">
    <div class="title_wrap">
        <h3>Opening this week</h3>
        <div class="search_bar">
            <input class="search_input" type="text" placeholder="Search for movie">
            <div class="search_icon_wrap">
                <i class="fa-solid fa-magnifying-glass search_icon"></i>
            </div>
        </div>
    </div>
    <hr>
</div>